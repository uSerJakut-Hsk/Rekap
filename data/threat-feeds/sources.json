  "data_quality": {
    "validation_rules": [
      {
        "type": "ip_address",
        "validation": "valid_ipv4_or_ipv6",
        "required_fields": ["ip", "type", "confidence", "first_seen"]
      },
      {
        "type": "domain",
        "validation": "valid_domain_format",
        "required_fields": ["domain", "type", "confidence", "first_seen"]
      },
      {
        "type": "hash",
        "validation": "valid_md5_sha1_sha256",
        "required_fields": ["hash", "malware_family", "confidence", "first_seen"]
      }
    ],
    "confidence_levels": {
      "high": "Verified through multiple sources or direct analysis",
      "medium": "Single reliable source or indirect evidence",
      "low": "Unverified report or automated detection"
    }
  },
  "update_schedule": {
